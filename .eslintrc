{
  "parser": "babel-eslint",

  "rules": {
    "arrow-spacing": 2,
    "curly": 2,
    "eol-last": 2,
    "indent": [1, 2],
    "no-lonely-if": 2,
    "no-spaced-func": 2,
    "no-unused-vars": 1,
    "no-var": 2,
    "prefer-const": 2,
    "quotes": [2, "double"],
    "space-before-blocks": 2

  }
}
